{
  "name": "graph",
  "description": "Outputs a Graphviz dot graph for the nodes in the node topology.",
  "examples": [
    "To graph all nodes:\n\n    $ driver node graph\n    "
  ],
  "flags": [
    {
      "kind": "Switch",
      "optionality": "optional",
      "long": "--help",
      "short": null,
      "description": "display usage information",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "only"
        }
      },
      "optionality": "optional",
      "long": "--only",
      "short": "o",
      "description": "filter the nodes by a criteria: bound, unbound, ancestors:<node_name>, primary_ancestors:<node_name>, descendants:<node_name>, relatives:<node_name>, primary_relatives:<node_name>, siblings:<node_name>, or primary_siblings:<node_name>. the primary variants indicate to only traverse primary parents when encountering composites",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "orientation"
        }
      },
      "optionality": "optional",
      "long": "--orientation",
      "short": "r",
      "description": "changes the visual orientation of the graph's nodes. Allowed values are \"lefttoright\"/\"lr\" and \"toptobottom\"/\"tb\".",
      "hidden": false
    },
    {
      "kind": "Switch",
      "optionality": "optional",
      "long": "--services",
      "short": null,
      "description": "whether to show an edge for service routes between nodes. It is highly recommended to narrow down the graph by using filters to just the nodes you need, otherwise the graph is extremely crowded and can cause timeouts on render engines.",
      "hidden": false
    },
    {
      "kind": "Switch",
      "optionality": "optional",
      "long": "--html",
      "short": null,
      "description": "generate an html to display a generated svg with interactivity like highlighting. Reads from stdin or from the given svg file.",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "svg"
        }
      },
      "optionality": "optional",
      "long": "--svg",
      "short": "s",
      "description": "the svg file to read for processing with the html generation.",
      "hidden": false
    }
  ],
